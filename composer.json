{
    "name": "muffin/slug",
    "description": "Slugging support for CakePHP ORM",
    "type": "cakephp-plugin",
    "keywords": [
        "cakephp",
        "muffin",
        "slug",
        "orm"
    ],
    "homepage": "https://github.com/usemuffin/slug",
    "license": "MIT",
    "authors": [
        {
            "name": "Jad Bitar",
            "homepage": "http://jadb.io",
            "role": "Author"
        },
        {
            "name": "ADmad",
            "homepage": "https://github.com/ADmad",
            "role": "Author"
        },
        {
            "name": "Others",
            "homepage": "https://github.com/usemuffin/slug/graphs/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/usemuffin/slug/issues",
        "source": "https://github.com/usemuffin/slug"
    },
    "require": {
        "cakephp/orm": "^5.0"
    },
    "require-dev": {
        "cakephp/cakephp": "5.0",
        "phpunit/phpunit": "^10.1",
        "cocur/slugify": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Muffin\\Slug\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Muffin\\Slug\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
